<div class="container">
    <h5><b>USERS</b></h5>
    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">username</th>
      <th scope="col">email</th>
      <th scope="col">phone</th>
    </tr>
  </thead>
  <tbody>
    {{#each this.parents}} 
    <tr>
      <th scope="row">{{id}}</th>
      <td>{{username}}</td>
      <td>{{email}}</td>
      <td>{{phone}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
